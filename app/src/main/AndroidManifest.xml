<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ingrid.projetointegrador_vic">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:exported="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:protectionLevel="signature"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:syncable="true"
        android:theme="@style/Theme.ProjetoIntegrador_Vic.NoActionBar">
        <activity
            android:name="com.ingrid.projetointegrador_vic.presentation.activity.SecondMovieDescriptionActivity"
            android:label="@string/title_activity_second_movie_description"
            android:theme="@style/Theme.ProjetoIntegrador_Vic.NoActionBar">

        </activity>
        <activity android:name="com.ingrid.projetointegrador_vic.presentation.activity.MovieActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>